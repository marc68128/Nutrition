<Application x:Class="Nutrition.Wpf.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Nutrition"
             xmlns:wpf="clr-namespace:Nutrition.Wpf">
    <Application.Resources>
        <ResourceDictionary>
            <wpf:InterfaceTemplateSelector x:Key="InterfaceTemplateSelector"></wpf:InterfaceTemplateSelector>

            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/Nutrition.Wpf.Views;component/Views/Home.xaml" />
                <ResourceDictionary Source="pack://application:,,,/Nutrition.Wpf.Views;component/Views/Aliment.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
